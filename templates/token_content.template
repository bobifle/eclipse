              <net.rptools.maptool.model.Token>
                <id>
                  <baGUID>{{token.guid}}</baGUID>
                </id>
                <beingImpersonated>false</beingImpersonated>
                <imageAssetMap>
                  <entry>
                    <null/>
                    <net.rptools.lib.MD5Key>
                      <id>{{token.icon.md5}}</id>
                    </net.rptools.lib.MD5Key>
                  </entry>
                </imageAssetMap>
                <x>{{token.x}}</x>
                <y>{{token.y}}</y>
                <z>2</z>
                <anchorX>0</anchorX>
                <anchorY>0</anchorY>
                <sizeScale>1.0</sizeScale>
                <lastX>0</lastX>
                <lastY>0</lastY>
                <snapToScale>{{token.snapToScale}}</snapToScale>
                <width>{{token.width}}</width>
                <height>{{token.height}}</height>
                <isoWidth>{{token.width}}</isoWidth>
                <isoHeight>{{token.height}}</isoHeight>
                <scaleX>1.0</scaleX>
                <scaleY>1.0</scaleY>
                <sizeMap>
                  <entry>
                    <java-class>net.rptools.maptool.model.SquareGrid</java-class>
                    <net.rptools.maptool.model.GUID>
                      <baGUID>{{token.size_guid}}</baGUID>
                    </net.rptools.maptool.model.GUID>
                  </entry>
                </sizeMap>
                <snapToGrid>{{token.snapToGrid}}</snapToGrid>
                <isVisible>true</isVisible>
                <visibleOnlyToOwner>false</visibleOnlyToOwner>
                <vblAlphaSensitivity>-1</vblAlphaSensitivity>
                <alwaysVisibleTolerance>2</alwaysVisibleTolerance>
                <isAlwaysVisible>false</isAlwaysVisible>
                <name>{{token.name}}</name>
                <ownerType>0</ownerType>
                <tokenShape>TOP_DOWN</tokenShape>
                <tokenType>{{token.type}}</tokenType>
                <layer>{{token.layer}}</layer>
                <propertyType>{{token.prop_type}}</propertyType>
                <tokenOpacity>1.0</tokenOpacity>
                <terrainModifier>1.0</terrainModifier>
                <isFlippedX>false</isFlippedX>
                <isFlippedY>false</isFlippedY>
{%-if token.portrait %}
                <portraitImage>
                  <id>{{ token.portrait.md5}}</id>
                </portraitImage>
{%- endif %}
                <sightType>Normal</sightType>
                <hasSight>{{token.hasSight}}</hasSight>
                <notes>{{token.notes|e}}</notes>
                <gmNotes>{{token.gmNotes|e}}</gmNotes>
                <state>
	{%- for state in token.states %}
                  <entry>
                    <string>{{state.name}}</string>
                    <boolean>{{state.value}}</boolean>
                  </entry>
	{%- endfor %}
                </state>
                <propertyMapCI>
                  <store>
	{%- for prop in token.props %}
	            {{prop.render()}}
	{%- endfor%}
                  </store>
                </propertyMapCI>
                <macroPropertiesMap>
{%for macro in token.macros %}
                  <entry>
                    <int>{{loop.index}}</int>
                    <net.rptools.maptool.model.MacroButtonProperties>
                      <saveLocation>Token</saveLocation>
                      <index>{{loop.index}}</index>
                      <colorKey>{{macro.color}}</colorKey>
                      <hotKey>None</hotKey>
                      <command>{{macro.command|e}}</command>
                      <label>{{macro.label}}</label>
                      <group>{{macro.group}}</group>
                      <sortby></sortby>
                      <autoExecute>true</autoExecute>
                      <includeLabel>false</includeLabel>
                      <applyToTokens>true</applyToTokens>
                      <fontColorKey>{{macro.fontColor}}</fontColorKey>
                      <fontSize>1.00em</fontSize>
                      <minWidth>90</minWidth>
                      <maxWidth></maxWidth>
                      <allowPlayerEdits>true</allowPlayerEdits>
                      <toolTip></toolTip>
                      <commonMacro>false</commonMacro>
                      <compareGroup>true</compareGroup>
                      <compareSortPrefix>true</compareSortPrefix>
                      <compareCommand>true</compareCommand>
                      <compareIncludeLabel>true</compareIncludeLabel>
                      <compareAutoExecute>true</compareAutoExecute>
                      <compareApplyToSelectedTokens>true</compareApplyToSelectedTokens>
                    </net.rptools.maptool.model.MacroButtonProperties>
                  </entry>
{%- endfor%}
                </macroPropertiesMap>
                <speechMap/>
              </net.rptools.maptool.model.Token>
